public class YourInitials

	public static void main( String[] args )
	{
		System.out.println( "       A       L      " );
		System.out.println( "      A A      L      " );
		System.out.println( "     A   A     L      " );
		System.out.println( "    AAAAAAA    L      " );
		System.out.println( "   A       A   L      " );
		System.out.println( "  A         A  LLLLLLL" );
	}
}